{
  "buildCommand": null,
  "outputDirectory": ".",
  "framework": null,
  "rewrites": [
    {
      "source": "/",
      "destination": "/html/home_page.html"
    },
    {
      "source": "/home",
      "destination": "/html/home_page.html"
    },
    {
      "source": "/tree",
      "destination": "/html/tree_page.html"
    },
    {
      "source": "/search",
      "destination": "/html/search_page.html"
    },
    {
      "source": "/profile",
      "destination": "/html/profile.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

